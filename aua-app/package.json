{
  "name": "aua-app",
  "version": "1.0.0",
  "description": "AUA server",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "up": "node index.js",
    "start:prod": "npm run link-web && npm run build && TZ=utc node index.js",
    "start": "TZ=utc ts-node index.ts",
    "dev": "npm run clean && nodemon -w src --ext ts --exec npm start",
    "build": "npm run lint && npm run clean && npm run compile",
    "build:prod": "rm -rf www && npm run compile",
    "clean": "ts-clean && rm -rf www",
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "link-web": "rm -rf www && ln -sfn \"$(pwd)/../aua-web/build\" www",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert",
    "migrate:new": "npm run typeorm migration:create -- -n",
    "migrate:gen": "ts-node typeorm migration:generate -n",
    "typeorm": "TS_NODE_PROJECT=tsconfig.ts-node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js"
  },
  "author": "techseeding2020@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.4",
    "ts-clean": "^1.0.3",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "aws-sdk": "^2.686.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "delay": "^4.4.0",
    "dotenv": "^8.2.0",
    "error-to-json": "^2.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-list-endpoints": "^4.0.1",
    "fs-extra": "^9.0.1",
    "http-assert": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "markdown-it": "^11.0.1",
    "markdown-it-pdf": "^1.1.0",
    "markdown-pdf": "^10.0.0",
    "moment": "^2.27.0",
    "pg": "^8.3.3",
    "sanitize-html": "^1.27.4",
    "serialize-error": "^7.0.1",
    "string-to-stream": "^3.0.1",
    "swagger-routes-express": "^3.1.2",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0",
    "ws": "^7.3.1",
    "yamljs": "^0.3.0"
  }
}
