{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@craco/craco": "^5.6.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.3.0",
    "axios": "^0.19.2",
    "craco-less": "^1.16.0",
    "file-saver": "^2.0.2",
    "history": "^5.0.0",
    "lodash": "^4.17.15",
    "mime-format": "^2.0.0",
    "moment": "^2.25.3",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-image-lightbox": "^5.1.1",
    "react-multi-carousel": "^2.5.5",
    "react-player": "^2.6.0",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "3.4.1",
    "react-to-print": "^2.9.0",
    "react-window-size": "^1.2.2",
    "styled-components": "^5.1.0",
    "uuid": "^8.3.0",
    "validator": "^13.1.1"
  },
  "scripts": {
    "start": "cross-env PORT=5006 REACT_APP_AUA_GIT_HASH=$(git rev-parse --short HEAD) craco start",
    "build": "npm run clean && cross-env CLIENT_ENV=development GENERATE_SOURCEMAP=true craco build",
    "build:prod": "npm run clean && npm run compile",
    "compile": "cross-env CLIENT_ENV=production GENERATE_SOURCEMAP=false REACT_APP_AUA_GIT_HASH=$(git rev-parse --short HEAD) craco build",
    "compile:prod": "cross-env CLIENT_ENV=production GENERATE_SOURCEMAP=false craco build",
    "clean": "rm -rf build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dev": "yarn && yarn build && yarn start",
    "watched": "nodemon -w src --ext js --exec npm run build",
    "watch": "npm-watch",
    "g": "npx generate-react-cli component --",
    "p": "npx generate-react-cli component -- --type=page",
    "upload": " aws s3 sync build s3://aua-web/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  }
}
